{
    "name": "@uppercod/exp-route",
    "type": "module",
    "version": "1.2.1",
    "description": "exp-route, designed to compare routes on the client based on simple expressions",
    "main": "./exp-route.cjs",
    "module": "./src/exp-route.js",
    "typings": "types.d.ts",
    "exports": {
        "import": "./src/exp-route.js",
        "require": "./exp-route.cjs"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "ava",
        "prepare": "npm run build && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/exp-route.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/exp-route/issues"
    },
    "homepage": "https://github.com/UpperCod/exp-route#readme",
    "devDependencies": {
        "@betit/rollup-plugin-rename-extensions": "^0.1.0",
        "@rollup/plugin-node-resolve": "^8.1.0",
        "ava": "^3.10.1",
        "builtin-modules": "^3.1.0",
        "rollup": "^2.35.1"
    },
    "ava": {
        "files": [
            "test/**/*.test.js"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    }
}
